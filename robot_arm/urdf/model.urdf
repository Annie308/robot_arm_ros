    </inertial>
  </link>

   <joint name="link1_to_link2" type="revolute">
   <axis xyz="0 1 0"/>
   <origin xyz="0 0 0" rpy="0 0 0"/>
   <limit effort="10" velocity="1" lower="-3.14" upper="3.14"/>
    <parent link="link1"/>
    <child link="link2"/>
  </joint>

  <link name="link3">
    <visual>
        <origin xyz="0.34 -0.03 0" rpy="-1.57 0 1.57" />
