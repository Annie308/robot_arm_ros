<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="arm">
    <link name = "axis">
    <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
    </visual>
    </link>

    <link name="base_link">
    <visual>
        <origin xyz="0 0 0" rpy="1.57 0 0" />
        <geometry>
          <mesh filename="package://robot_arm/meshes/base.stl" scale="0.01 0.01 0.01"/>
        </geometry>
        <material name="grey">
        <color rgba="0.83 0.83 0.83 1.0"/>
      </material>
    </visual>
    <collision>
    </collision>
    </link>

  <joint name="axis_to_base" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0"/> 
    <parent link="axis"/>
    <child link="base_link"/>
  </joint>

  <link name="link1">
    <visual>
     <origin xyz="0 0 0" rpy="1.57 0 1.57" />
        <geometry>  
         <mesh filename="package://robot_arm/meshes/link1.stl" scale="0.01 0.01 0.01"/>
        </geometry>
         <material name="grey">
        <color rgba="0.83 0.83 0.83 1.0"/>
      </material>
    </visual>
    <inertial>
      <mass value="10"/>
      <inertia ixx="1e-3" ixy="0.0" ixz="0.0" iyy="1e-3" iyz="0.0" izz="1e-3"/>
    </inertial>
  </link>

   <joint name="base_to_link1" type="revolute">
   <axis xyz="0 0 1"/>
   <origin xyz="0 0 0.55" rpy="0 0 0"/>
   <limit effort="10" velocity="1" lower="-3.14" upper="3.14"/>
    <parent link="base_link"/>
    <child link="link1"/>
  </joint>

 
  <link name="link2">
    <visual>
        <origin xyz="1 -0.23 0.4" rpy="-1.57 0 1.57" />
